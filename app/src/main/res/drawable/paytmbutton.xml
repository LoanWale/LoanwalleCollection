<?xml version="1.0" encoding="utf-8"?><ripple xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:color="#ffff"    tools:ignore="NewApi">    <item android:id="@android:id/mask"        tools:ignore="NewApi">        <shape android:shape="rectangle">            <solid android:color="#ffff" />            <corners android:radius="5dp" />        </shape>    </item>    <item android:id="@android:id/background">        <shape android:shape="rectangle">            <gradient                android:angle="90"                android:endColor="@color/applColor"                android:startColor="@color/applColor"                android:type="linear" />            <corners android:radius="5dp" />        </shape>    </item></ripple>
        <activity android:name=".ui.main.view.Activity.RemarksActivity" />
        <activity android:name=".ui.main.view.Activity.ResidanceActivity" />
        <activity
            android:name=".ui.main.view.Activity.Verification"
            android:exported="true" />
        <activity
            android:name=".ui.main.view.Activity.PreviousPayment"
            android:exported="true" />
        <activity
            android:name=".ui.main.view.Activity.Total_Leads"
            android:exported="true" />

        <activity
            android:name=".ui.main.view.Activity.Collection_Profile"
